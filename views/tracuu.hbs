<script>
	function list_all_service1() {

		document.getElementById("data_table_service").innerHTML = "";

		list_all_service((result) => {
			var birds = result;
			console.log(result);
			var ele = document.getElementById('data_table_service');
			for (var i = 0; i < birds.payload.length; i++) {
				ele.innerHTML = ele.innerHTML + "<tr>" +
					"<td hidden>" + birds.payload[i].id + "</td>" +
					"<td>" + birds.payload[i].Ten + "</td>" +
					"<td>" + birds.payload[i].GiaTien + "</td></tr>";
			}
			/*"<a class=\"bi-cart-fill me-1\" onclick=\"add_service_tocart(" + birds.payload[0].id + ")\" id=\"detai\" >Chọn</a>" +*/
			document.getElementById("data_table_service").innerHTML = ele.innerHTML;


		});
	}
</script>

